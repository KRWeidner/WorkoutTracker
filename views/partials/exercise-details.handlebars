<div id="exerTitle">
  <h2>{{workoutName}}</h2>
</div>
<div class="exerData">
  <div class="exerSRep">
    <a class="SRepBox">Sets left: {{sets}}</a>
    <a class="SRepBox">Reps left: {{reps}}</a>
  </div>
  <div class="exerImg">

      {{#each exerciseimages as |exerciseimage| }}
      <img src= "/images/bootyworkout-donkeyKicks-istockphoto-1306046535-1024x1024.jpg" alt="{{exerciseimage.photo_link}}" />
      {{/each}}

  </div>
</div>

